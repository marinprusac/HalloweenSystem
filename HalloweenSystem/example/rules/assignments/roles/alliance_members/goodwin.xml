<?xml version="1.0" encoding="utf-8"?>
<rule name="Assign goodwin">
    <actions>
        <assign>
            <players>
                <complement>
                    <iterate name="tag" parameter_type="tag">
                        <parameter_selector>
                            <group_tag name="Alliance members"/>
                        </parameter_selector>
                        <iterable_selector>
                            <has_tag>
                                <tags>
                                    <parameter name="tag"/>
                                </tags>
                            </has_tag>
                        </iterable_selector>
                    </iterate>
                </complement>
            </players>
            <tags>
                <tag name="Goodwin"/>
            </tags>
        </assign>
    </actions>
</rule>